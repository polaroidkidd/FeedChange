{
	"name": "feedchange",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"description": "A simple, privacy-focused baby tracking application for monitoring feeding sessions and diaper changes. Built with SvelteKit and designed with parents' privacy in mind.",
	"author": {
		"name": "Daniel L. Einars",
		"email": "feedchange@dle.dev"
	},
	"license": "MIT",
	"scripts": {
		"dev": "pnpm db:push:dev && dotenv -e .env.local -- vite dev --host",
		"build": "pnpm clean && pnpm db:generate:prod && vite build --mode production",
		"preview": "pnpm build && wrangler pages dev ./.svelte-kit/cloudflare",
		"clean": "rimraf dist .svelte-kit",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"cf:deploy": "pnpm build && wrangler pages deploy --branch=master",
		"db:generate:dev": "dotenv -e .env.local -- prisma generate",
		"db:generate:prod": "dotenv -e .env -- prisma generate",
		"db:push:dev": "pnpm db:generate:dev && dotenv -e .env.local -- prisma db push",
		"db:push:prod": "pnpm db:generate:prod && dotenv -e .env -- prisma db push"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.29.0",
		"@prisma/adapter-d1": "^6.10.1",
		"@prisma/client": "6.10.1",
		"@sveltejs/adapter-cloudflare": "^7.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^20",
		"@vitest/browser": "^3.2.3",
		"dotenv": "^16.5.0",
		"dotenv-cli": "^8.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-css": "^0.11.0",
		"eslint-plugin-prettier": "^5.5.1",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-svelte": "^3.10.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"eslint-plugin-yml": "^1.18.0",
		"globals": "^16.2.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-eslint": "^16.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.10.1",
		"rimraf": "^6.0.1",
		"sass-embedded": "^1.89.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-confetti": "^2.0.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.35.0",
		"vite": "^6.2.6",
		"vite-plugin-devtools-json": "^0.2.0",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0",
		"wrangler": "^4.22.0",
		"yaml-eslint-parser": "^1.3.0"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.0.0",
		"@libsql/client": "^0.14.0",
		"@prisma/adapter-better-sqlite3": "^6.10.1",
		"@tanstack/svelte-query": "^5.81.5",
		"clsx": "^2.1.1",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.8.2",
		"flowbite-svelte-icons": "^2.2.1",
		"sveltekit-rate-limiter": "^0.7.0",
		"unique-names-generator": "^4.7.1",
		"uuid": "^11.1.0",
		"valibot": "^1.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"@tailwindcss/oxide",
			"better-sqlite3",
			"esbuild",
			"prisma",
			"sharp",
			"workerd"
		],
		"patchedDependencies": {
			"flowbite-svelte": "patches/flowbite-svelte.patch"
		}
	},
	"packageManager": "pnpm@10.12.4",
	"engines": {
		"node": ">=v22.9.0",
		"pnpm": ">=10.12.4"
	}
}
